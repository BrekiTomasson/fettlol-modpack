{
	"credit": "Created by Mickey Joe Alpha - https://www.Reddit.com/user/MushirMickeyJoe",
    "multipart": [
        {   "apply": { "model": "block/oak_fence_ends" }
		},
        {   "when": { "north": "false" },
            "apply": { "model": "block/oak_fence_post" }
        },
        {   "when": { "east": "false" },
            "apply": { "model": "block/oak_fence_post", "y": 90 }
        },
        {   "when": { "south": "false" },
            "apply": { "model": "block/oak_fence_post", "y": 180 }
        },
        {   "when": { "west": "false" },
            "apply": { "model": "block/oak_fence_post", "y": 270 }
        },
        {   "when": { "north": "true" },
            "apply": { "model": "block/oak_fence_rails" }
        },
        {   "when": { "east": "true" },
            "apply": { "model": "block/oak_fence_rails", "y": 90 }
        },
        {   "when": { "south": "true" },
            "apply": { "model": "block/oak_fence_rails", "y": 180 }
        },
        {   "when": { "west": "true" },
            "apply": { "model": "block/oak_fence_rails", "y": 270 }
        }
    ]
}
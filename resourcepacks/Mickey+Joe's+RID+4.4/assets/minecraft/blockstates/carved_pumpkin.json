              "apply": [
		    { "model": "block/carved_pumpkin_0", "y": 270 },
            { "model": "block/carved_pumpkin_1", "y": 270 }
			]
        },
		{     "when": { "facing": "east" },
              "apply": [
		    { "model": "block/carved_pumpkin_0", "y": 90 },
